datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id    String   @id @default(cuid())
  userName  String
  email String  @unique
  avatarURL String
  status String
  password String
}
