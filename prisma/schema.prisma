datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id    String   @id @default(cuid())
  email String  @unique
  avatarURL String?
  status String?
  password String
  firstName String?
  lastName String?
  phoneNumber String?
  updatedAt   DateTime @updatedAt
  createdAt   DateTime @default(now())
}
